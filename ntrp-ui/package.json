{
  "name": "ntrp-ui",
  "version": "0.1.0",
  "description": "Terminal UI client for ntrp",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ntrp-ui": "./dist/index.js"
  },
  "scripts": {
    "dev": "node --max-old-space-size=8192 node_modules/.bin/tsx src/index.tsx",
    "build": "tsc",
    "start": "node --max-old-space-size=8192 dist/index.js"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@pppp606/ink-chart": "^0.2.4",
    "cli-markdown": "^3.5.1",
    "ink": "^6.6.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.8",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
